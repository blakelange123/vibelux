{
  "name": "vibelux-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "echo 'Starting build...' && next build",
    "build:vercel": "next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "optimize": "./optimize-build.sh",
    "clean": "rm -rf .next node_modules/.cache out dist",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:api": "jest --testPathPattern=__tests__/api",
    "test:lib": "jest --testPathPattern=__tests__/lib",
    "test:unit": "jest --testPathPattern=src/__tests__/unit",
    "test:integration": "jest --testPathPattern=src/__tests__/integration",
    "test:e2e": "playwright test",
    "test:load": "k6 run tests/load/load-test.js",
    "test:smoke": "playwright test --grep @smoke",
    "test:ai": "node test-ai-setup.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed-demo": "npx tsx src/scripts/seed-demo-data.ts",
    "db:reset-demo": "npx prisma db push --force-reset && npm run db:seed && npm run db:seed-demo",
    "backup:create": "tsx src/scripts/create-backup.ts",
    "backup:restore": "tsx src/scripts/restore-backup.ts",
    "security:audit": "npm audit && npx better-npm-audit audit",
    "security:check": "npx snyk test",
    "analyze": "cross-env ANALYZE=true next build",
    "lighthouse": "lhci autorun",
    "validate:env": "tsx scripts/validate-environment.ts",
    "start:cron": "tsx -e \"import { getCronRunner } from './src/services/cron-runner'; getCronRunner().start()\"",
    "stop:cron": "tsx -e \"import { getCronRunner } from './src/services/cron-runner'; getCronRunner().stop()\"",
    "start:energy-monitoring": "tsx -e \"import { EnergyMonitoringService } from './src/services/energy-monitoring-service'; new EnergyMonitoringService().start()\"",
    "test:influxdb": "tsx test-influxdb.ts",
    "setup:revenue-sharing": "npm run validate:env && npm run db:generate && echo 'ðŸŽ‰ Revenue sharing setup complete!'",
    "deploy:update": "tsx scripts/deploy-update.ts",
    "increase:rollout": "tsx scripts/deploy-update.ts increase-rollout",
    "emergency:disable": "tsx scripts/deploy-update.ts emergency-disable",
    "migrate:production": "npx prisma migrate deploy",
    "backup:production": "tsx scripts/backup-production.ts"
  },
